# Main library - 'cstring.X.a'

$(LIB_CORE_DEBUG): $(OBJ_CORE_DEBUG) $(PROJ_MAKEFILE_NAME)
	@-if not exist $(PROJ_LIB_DIR)\NUL mkdir $(PROJ_LIB_DIR)
	$(AR) $(AR_ARGS) $@ $(OBJ_CORE_DEBUG)

$(OBJ_lib_core_DEBUG):	$(PROJ_MAKEFILE_NAME) $(PROJ_SRC_DIR)\cstring.core.c
	$(CC) $(CC_ARGS_DEBUG) $(CC_INCLUDES) --output-path=$@ $(PROJ_SRC_DIR)\cstring.core.c
$(OBJ_lib_vect_DEBUG):	$(PROJ_MAKEFILE_NAME) $(PROJ_SRC_DIR)\cstring.vector.c
	$(CC) $(CC_ARGS_DEBUG) $(CC_INCLUDES) --output-path=$@ $(PROJ_SRC_DIR)\cstring.vector.c

$(LIB_CORE_RELEASE): $(OBJ_CORE_RELEASE) $(PROJ_MAKEFILE_NAME)
	@-if not exist $(PROJ_LIB_DIR)\NUL mkdir $(PROJ_LIB_DIR)
	$(AR) $(AR_ARGS) $@ $(OBJ_CORE_RELEASE)

$(OBJ_lib_core_RELEASE):	$(PROJ_MAKEFILE_NAME) $(PROJ_SRC_DIR)\cstring.core.c
	$(CC) $(CC_ARGS_RELEASE) $(CC_INCLUDES) --output-path=$@ $(PROJ_SRC_DIR)\cstring.core.c
$(OBJ_lib_vect_RELEASE):	$(PROJ_MAKEFILE_NAME) $(PROJ_SRC_DIR)\cstring.vector.c
	$(CC) $(CC_ARGS_RELEASE) $(CC_INCLUDES) --output-path=$@ $(PROJ_SRC_DIR)\cstring.vector.c

